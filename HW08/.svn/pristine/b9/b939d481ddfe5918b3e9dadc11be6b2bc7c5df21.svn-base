package model.algo;

import javax.swing.ImageIcon;

import common.DataPacketAlgoCmd;
import model.cmd.DisplayImageCmd;
import model.cmd.DisplayTextCmd;
import model.cmd.TypeJoinChatRoom;
import provided.datapacket.DataPacketAlgo;

/**
 * The data packet algo for chat room app.
 *
 */
public class DataPacketChatRoomAlgo extends DataPacketAlgo<Object,Object> {

	/**
	 * 
	 */
	private static final long serialVersionUID = 8119780355923674125L;

	/**
	 * Constructor.
	 * @param defaultCmd data packet default command.
	 */
	public DataPacketChatRoomAlgo(DataPacketAlgoCmd<?> defaultCmd) {
		super(defaultCmd);
		setCmd(ImageIcon.class, new DisplayImageCmd());
		setCmd(String.class, new DisplayTextCmd());
		setCmd(TypeJoinChatRoom.class, new DisplayImageCmd());
	}
}
