          <defaultValue xmi:type="uml:LiteralString" xmi:id="_pdyDAZgFEeePNM5TF4gU1g" value="new Randomizer()"/>
        </ownedAttribute>
        <interfaceRealization xmi:id="_pcg3sJgFEeePNM5TF4gU1g" client="_pZW2AJgFEeePNM5TF4gU1g" supplier="_pZD7EJgFEeePNM5TF4gU1g" contract="_pZD7EJgFEeePNM5TF4gU1g"/>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_pd-3VJgFEeePNM5TF4gU1g" name="randomLoc" specification="_pdS60JgFEeePNM5TF4gU1g">
