        </ownedBehavior>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_pd-3V5gFEeePNM5TF4gU1g" name="randomLoc" specification="_pdTh4JgFEeePNM5TF4gU1g">
          <ownedParameter xmi:id="_pd-3WJgFEeePNM5TF4gU1g" name="dim" type="_pdK_AZgFEeePNM5TF4gU1g"/>
          <ownedParameter xmi:id="_pd-3WZgFEeePNM5TF4gU1g" type="_pbsYUZgFEeePNM5TF4gU1g" direction="return"/>
          <language>java</language>
          <body>return (new Point(randomInt(0, dim.width), randomInt(0, dim.height)));&#xD;
	</body>
        </ownedBehavior>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_pd_eYJgFEeePNM5TF4gU1g" name="randomInt" specification="_pZh1IJgFEeePNM5TF4gU1g">
          <ownedParameter xmi:id="_pd_eYZgFEeePNM5TF4gU1g" name="min">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
          </ownedParameter>
          <ownedParameter xmi:id="_pd_eYpgFEeePNM5TF4gU1g" name="max">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
          </ownedParameter>
          <ownedParameter xmi:id="_pd_eY5gFEeePNM5TF4gU1g" direction="return">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
