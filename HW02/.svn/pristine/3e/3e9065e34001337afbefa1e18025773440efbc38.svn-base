          <ownedParameter xmi:id="_pd-3VpgFEeePNM5TF4gU1g" type="_pbsYUZgFEeePNM5TF4gU1g" direction="return"/>
          <language>java</language>
          <body>return (new Point(randomInt(0, rect.width), randomInt(0, rect.height)));&#xD;
